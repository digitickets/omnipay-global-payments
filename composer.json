{
    "name": "digitickets/omnipay-global-payments",
    "type": "library",
    "description": "Custom OmniPay driver for Global Payments",
    "keywords": ["omnipay", "globalpayments"],
    "homepage": "https://github.com/digitickets/omnipay-global-payments",
    "autoload": {
        "psr-4": { "Omnipay\\GlobalPayments\\" : "src/" }
    },
    "config": {
        "platform": {
            "php": "7.0"
        }
    },
    "require": {
        "php": "^7.0",
        "omnipay/common": "~2.0",
        "league/iso3166": "^2.1",
        "giggsey/libphonenumber-for-php": "8.13.*",
        "globalpayments/php-sdk": "^3.1"
    },
    "require-dev": {
        "omnipay/tests": "~2.0",
        "squizlabs/php_codesniffer": "~1.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x.x-dev"
        }
    },
    "prefer-stable": true
}
